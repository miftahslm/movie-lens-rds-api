{
  "name": "movie-lens-rds-api",
  "version": "1.0.0",
  "description": "Large-Scale Data Management System using RDS and RESTful API",
  "main": "server.js",
  "scripts": {
  "start": "node server.js"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db-minimal": "node scripts/init-db-minimal.js",
    "test:performance": "k6 run tests/performance-test.js",
    "test:concurrent": "k6 run tests/concurrent-test.js --out json=results/concurrent.json",
    "test:large-data": "k6 run tests/large-data-test.js --out json=results/large-data.json",
    "test:all": "npm run test:performance && npm run test:concurrent && npm run test:large-data",
    "check-total": "node scripts/check-total.js",
    "test:remote": "k6 run tests/remote-performance-test.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.913.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "node-cache": "^5.1.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }

}
